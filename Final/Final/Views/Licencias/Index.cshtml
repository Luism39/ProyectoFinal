@model IEnumerable<Final.Views.Licencias>

@{
    ViewBag.Title = "Index";
}

<h2>Licencia de empleados</h2>
<button type="button" class="btn btn-light" onclick="location.href='@Url.Action("Create","Licencias")'">Crear Licencia</button>
<br />
<table class="table table-bordered">
    <tr class="btn-info">
        <th>
            @Html.DisplayNameFor(model => model.Desde)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hasta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Motivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comentarios)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Empleados)
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Desde)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hasta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Motivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comentarios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empleados)
            </td>

        </tr>
    }

</table>


